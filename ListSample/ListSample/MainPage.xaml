<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:local="clr-namespace:ListSample" x:Class="ListSample.MainPage">
    <StackLayout>
        <!-- Place new controls here -->
        <Label Text="Welcome to Xamarin.Forms!" HorizontalOptions="Center" VerticalOptions="CenterAndExpand" />
        <ListView>
      <ListView.ItemsSource>
          <x:Array Type="{x:Type x:String}">
            <x:String>Bears</x:String>
            <x:String>Lions</x:String>
            <x:String>Packers</x:String>
            <x:String>Vikings</x:String>
          </x:Array>
      </ListView.ItemsSource>
 
</ListView>
<Label Margin="10" Text="Cool"></Label>
  <ListView Margin="10" x:Name="TeamListOut">
    <ListView.ItemTemplate>
      <DataTemplate>
        <TextCell Text="{Binding Mascot}" />
      </DataTemplate>
    </ListView.ItemTemplate>
  </ListView>
    </StackLayout>
</ContentPage>